// example configuration file 
// for CRaneLab

{
    // what order to test detectors in 
    "detector-sequence"     : [1,2,3,4], 
    "measurement-time"      : 4, // time in minutes
    "paking-times"          : [500,1000,4000,5000], // peaking time in ns
    "save-waveforms"        : 1, // save waveform data,
    "use-chamber"           : 1, // use SUN climate chamber
    "chamber-target-temp"   : -37, // at what temperature we want the measurement
    "config-file"           : "config_x.json" // file with settings for the digitizer
}
